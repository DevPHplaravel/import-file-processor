{
    "name": "alessandro-mineo/import-file-processor",
    "description": "Un pacchetto per processare file CSV, Excel, JSON e XML con log e gestione degli errori.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AlessandroMineo\\ImportFileProcessor\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "maatwebsite/excel": "^3.1",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DevPHplaravel/import-file-processor.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
